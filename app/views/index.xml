<Alloy>
	<TabGroup>
		<Tab title="Pending" icon="KS_nav_ui.png">
			<Window title="Pending Guests">
				<TableView id="pending_table" dataCollection="guest" dataFilter="filterPending">
					<Require src="row_pending"/>
				</TableView>
			</Window>
		</Tab>
		<Tab title="Checked in" icon="KS_nav_views.png">
			<Window title="Checkin Guests">
				<TableView id="checkedin_table" dataCollection="guest" dataFilter="filterCheckedIn">
					<Require src="row_checked"/>
				</TableView>
			</Window>
		</Tab>
	</TabGroup>
</Alloy>